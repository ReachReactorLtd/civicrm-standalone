<?xml version="1.0"?>
<extension key="gvote_connection" type="module">
  <file>gvote_connection</file>
  <name>gvote_connection</name>
  <description>Connection between CiviCRM and GVote.</description>
  <license>AGPL-3.0</license>
  <authors>
    <author>
      <name>ReachReactor Ltd.</name>
      <email>app@reachreactor.ca</email>
      <role>Maintainer</role>
    </author>
  </authors>
  <urls>
    <url desc="Main Extension Page">https://github.com/ReachReactorLtd/civicrm-standalone</url>
    <url desc="Documentation">https://github.com/ReachReactorLtd/civicrm-standalone</url>
    <url desc="Support">https://github.com/ReachReactorLtd/civicrm-standalone</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-10-30</releaseDate>
  <version>1.0.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.80</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/GvoteConnection</namespace>
    <format>24.09.1</format>
    <angularModule>crmGvoteConnection</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty@1.0.3</mixin>
  </mixins>
</extension>
